<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<!-- Strutsの詳細設定 -->

<struts>
	<constant name="struts.devMode" value="true" />
	<constant name="struts.ui.theme" value="simple" />

	<package name="com.internousdev.amazoon.action" extends="struts-default">


		<action name="BirdAction" class="com.internousdev.amazoon.action.BirdAction"
			method="execute">
			<result name="success">bird.jsp</result>
		</action>

		<action name="BuyBirdAction" class="com.internousdev.amazoon.action.BuyBirdAction"
			method="execute">
			<result name="success">cart.jsp</result>
		</action>

		<action name="BuyCrocoAction" class="com.internousdev.amazoon.action.BuyCrocoAction"
			method="execute">
			<result name="success">cart.jsp</result>
		</action>

		<action name="BuyRionAction" class="com.internousdev.amazoon.action.BuyRionAction"
			method="execute">
			<result name="success">cart.jsp</result>
		</action>

		<action name="CartAction" class="com.internousdev.amazoon.action.CartAction"
			method="execute">
			<result name="success">cart.jsp</result>
		</action>


		<action name="CrocoAction" class="com.internousdev.amazoon.action.CrocoAction"
			method="execute">
			<result name="success">croco.jsp</result>
		</action>


		<action name="GoLoginAction" class="com.internousdev.amazoon.action.GoLoginAction"
			method="execute">
			<result name="success">login.jsp</result>
		</action>

		<action name="HomeAction" class="com.internousdev.amazoon.action.HomeAction"
			method="execute">
			<result name="success">animal.jsp</result>
		</action>

		<action name="LoginAction" class="com.internousdev.amazoon.action.LoginAction"
			method="execute">
			<result name="success">animal.jsp</result>
			<result name="error">login.jsp</result>
		</action>

		<action name="LogoutAction" class="com.internousdev.amazoon.action.LogoutAction"
			method="execute">
			<result name="success">logout.jsp</result>
		</action>

		<action name="MyPageAction" class="com.internousdev.amazoon.action.MyPageAction"
			method="execute">
			<result name="success">mypage.jsp</result>
		</action>



		<action name="RionAction" class="com.internousdev.amazoon.action.RionAction"
			method="execute">
			<result name="success">rion.jsp</result>
		</action>

		<action name="ThanksAction" class="com.internousdev.amazoon.action.ThanksAction"
			method="execute">
			<result name="success">thanks.jsp</result>
		</action>

		<action name="UserCreateAction" class="com.internousdev.amazoon.action.UserCreateAction"
			method="execute">
			<result name="success">userCreate.jsp</result>
		</action>

		<action name="UserCreateConfirmAction"
			class="com.internousdev.amazoon.action.UserCreateConfirmAction"
			method="execute">
			<result name="success">userCreateConfirm.jsp</result>
			<result name="error">userCreate.jsp</result>
		</action>

		<action name="UserCreateCompleteAction"
			class="com.internousdev.amazoon.action.UserCreateCompleteAction"
			method="execute">
			<result name="success">userCreateComplete.jsp</result>
		</action>


	</package>
</struts>